{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "note",
      "camelcase": "Note",
      "title": "Note",
      "scope": "source.note",
      "file-types": [
        "note"
      ],
      "injection-regex": "^note$",
      "class-name": "TreeSitterNote"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Note grammar for tree-sitter",
    "authors": [
      {
        "name": "coc"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-note"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}